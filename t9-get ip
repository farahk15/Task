<!DOCTYPE html>
<html>
  <body>
 <button onclick="getIp()">Get ip</button>
      <script>
        function getIp(){
          fetch('https://api.ipify.org?format=json')
          .then(response=>response.json())
          .then(data=>{
            console.log("your ip address", data.ip);
          })
          .catch(error=>{
            console.error("error",error);
          });
        }
       
      </script>
  </body>
</html>
